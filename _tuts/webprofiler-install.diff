+      'php_version' => PHP_VERSION,
+      'xdebug_enabled' => extension_loaded('xdebug'),
+      'xhprof_enabled' => extension_loaded('xhprof'),
+      'eaccel_enabled' => extension_loaded('eaccelerator') && ini_get('eaccelerator.enable'),
