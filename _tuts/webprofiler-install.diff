+
+  /**
+   * @param \Drupal\Core\Config\ConfigFactoryInterface $config_factory
+   * @param Profiler $profiler
+   * @param \Drupal\webprofiler\Profiler\ProfilerStorageManager $storageManager
+   * @param array $templates
