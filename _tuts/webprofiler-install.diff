+/**
+ * Deletes all files in a dir.
+ */
+function devel_empty_dir($dir) {
+  foreach (new DirectoryIterator($dir) as $file_info) {
+    if ($file_info->isFile()) {
+      unlink($file_info->getPathname());
+    }
+  }
+}
