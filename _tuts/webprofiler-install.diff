+                            <div class="wp-query-arguments">
+                                <table class="table--duo">
+                                    <tr>
+                                        <th>{{ 'Tags'|t }}</th>
+                                        <td><%- (tags) ? tags : '-' %></td>
+                                    </tr>
+                                    <tr>
+                                        <th>{{ 'Depends'|t }}</th>
+                                        <td><%- (depends) ? depends : '-' %></td>
+                                    </tr>
+                                </table>
+                            </div>
+                        </div>
+                        <% } %>
+                    </div>
+                    <% }); %>
